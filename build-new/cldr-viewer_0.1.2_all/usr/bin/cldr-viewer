#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

try:
    from cldr_viewer.main import main
    main()
except ImportError:
    try:
        from cldr_viewer.app import main
        main()
    except ImportError:
        print("Error: Cannot find main module for cldr-viewer", file=sys.stderr)
        sys.exit(1)
